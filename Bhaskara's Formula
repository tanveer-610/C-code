#include<stdio.h>
#include<math.h>
int main()
{
    float a,b,c,d,r1,r2,e;
    scanf("%f %f %f",&a,&b,&c);

    d=(b*b)-(4*a*c);
    e=2*a;

    if(d<0 || e==0)
    {
        printf("Impossivel calcular\n");
    }
    else
    {
    r1=(-b+(sqrt(d)))/e;
    r2=(-b-(sqrt(d)))/e;

    printf("R1= %.5f\nR2= %.5f\n",r1,r2);
    }


    return 0;
}
