#include<stdio.h>
int main()
{
    int i,a,b,c,ara[65],j=1,k;
    scanf("%d %d",&a,&b);
    while(a!=0 && b!=0)
    {
        for(i=0; i<a-1; i++)
        {
            scanf("%d",&ara[i]);
        }
        for(i=0; i<a-1; i++)
        {
            if(ara[i]==b)
               {
                 c=i;
                 k=c+1;

               }
        }
        if(i==a-1)
        { printf("CASE# %d\n%d not found\n",j,b);

        }
        else
        {

printf("CASE# %d\n%d found at %d\n",j,b,k);
        }
        j++;
        scanf("%d%d",&a,&b);
    }
}
