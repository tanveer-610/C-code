#include<stdio.h>
struct Person
{
    char name[20];
    int age;
    float salary;
};
void display(struct Person p)
{
    printf("\nName:%s\n",p.name);
    printf("Age:%d\n",p.age);
    printf("Salary:%f\n",p.salary);
}
int main()
{
    struct Person person1,person2;
    strcpy(person1.name,"anisul islam");
    person1.age=21;
    person1.salary=220135.02;
    display(person1);
     strcpy(person2.name,"tanvirul islam");
    person2.age=22;
    person2.salary=2203135.02;
    display(person2);
    getch();
}
